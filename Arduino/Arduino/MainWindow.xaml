<Window x:Class="ArduinoControl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Arduino Control" Height="400" Width="400">
    <StackPanel Margin="20">
        <Button x:Name="LedOnButton4" Content="LED 4 on " Click="LedOnButton4_Click" Margin="5"/>
        <Button x:Name="LedOffButton4" Content="LED 4 off" Click="LedOffButton4_Click" Margin="5"/>
        <Button x:Name="LedOnButton5" Content="LED 5 on" Click="LedOnButton5_Click" Margin="5"/>
        <Button x:Name="LedOffButton5" Content="LED 5 off" Click="LedOffButton5_Click" Margin="5"/>
        <TextBlock x:Name="ButtonStatusText" Text="Button not pressed" Margin="5"/>
    </StackPanel>
</Window>
